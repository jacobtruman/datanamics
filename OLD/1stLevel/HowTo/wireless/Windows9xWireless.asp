<%
ptype = "Windows 9x Wireless"
%>
<HEAD>
<Title>
	<%=ptype%>
</Title>
<LINK Rel='stylesheet' Type='text/css' HREF='/support_web/ows.css'>
</HEAD>
<body marginwidth='0' marginheight='0' scroll='yes' vlink='#0000FF' alink='#0000FF'>
	<table class='ms-main' cellpadding='0' cellspacing='0' border='0' width='100%' height='100%'>
		<TR valign='top'>
			<TD WIDTH='100%' colspan='3'>
				<table class='ms-bannerframe' border='0' cellspacing='0' cellpadding='3' width=100%>
					<tr>
						<td nowrap valign='middle' align='left' height='30'>
						</td>
					</tr>
				</table>
			</TD>
		</TR>
		<tr>
			<td valign='top' colspan='3'>
			<!--#include virtual='/support_web/Components/mainheader2.asp'-->
			</td>
		</tr>
		<tr>
			<td width='118' valign='top' rowspan='2' bgcolor='#F0F0F0'>
<!--#include virtual='/support_web/components/level1howtomenu.asp'-->
    </td>
    <td width='582' height='10' bgcolor='#D8D8D8' align='center'>
    <font size='4' face='Times New Roman'>
		<%=ptype%>
	</font>
    </td>
  </tr>
  <tr>
    <td valign='top'>
	<center>
	<BR>
    <font color='#0000FF' face='Verdana' size='2'>
		<a href='/support_web/1stlevel/HowTo/HowTo.asp'>
			HOME
		</a>
	</font>
	</center>
	<BR>
	<BR>
	<font color='#000000'>
		<B>
			1.
		</B>
		Double Click on
		<font color='blue'>
			My Computer.
		</font>
		<BR>
		<BR>
		<B>
			2.
		</B>
		Double Click on
		<font color='blue'>
			Control Panel.
		</font>
		<BR>
		<BR>
		<B>
			3.
		</B>
		Double Click on
		<font color='blue'>
			Network.
		</font>
		<BR>
		<BR>
		<B>
			4.
		</B>
		In the
		<font color='blue'>
			Network
		</font>
		view select your 802.11b network adapter.
		<BR>
		<BR>
		<B>
			5.
		</B>
		Click on
		<font color='blue'>
			Properties
		</font>
		<BR>
		-Select the
		<font color='blue'>
			Advanced
		</font>
		tab, select SSID, and type the SSID in the field on the right.
		<B>
			OR
		</B>
		<BR>
		-Select the
		<font color='blue'>
			Settings
		</font>
		tab and type the SSID in the SSID field.
		<B>
			OR
		</B>
		<BR>
		-Select the
		<font color='blue'>
			Basic
		</font>
		tab and enter the SSID in the field asking for the network name.
		<BR>
		<BR>
		<B>
			6.
		</B>
		Click
		<font color='blue'>
			OK
		</font>
		on the adapter screen.
		<BR>
		<BR>
		<B>
			7.
		</B>
		Click
		<font color='blue'>
			OK
		</font>
		on the Network Properties screen.
		<BR>
		<BR>
		<B>
			8.
		</B>
		Restart your computer.
		<BR>
		<BR>
		<B>
			<font color='red'>
				NOTE.
		</B>
		You may need to set the
		<font color='blue'>
			Channel Number
		</font>
		on some cards.
		<font>
		<BR>
		<BR>
	</font>
    </td>
  </tr>
</table>
</body>